.xans-myshop-asyncbankbook { padding:15px 30px 15px 0; margin:0 0 20px; border:5px solid #e8e8e8; background:#fff url("http://img.echosting.cafe24.com/skin/base/common/bg_division.gif") repeat-y 47% 0; }
.xans-myshop-asyncbankbook ul { margin:0 0 0 45px; }
.xans-myshop-asyncbankbook ul:after { content:""; display:block; clear:both; }
.xans-myshop-asyncbankbook ul li { float:left; width:49%; height:20px; font-size:12px; line-height:1.6; margin:5px 0; vertical-align:top; }
.xans-myshop-asyncbankbook .title { float:left; width:30%; padding:0 0 0 10px; font-weight:normal; background:url("http://img.echosting.cafe24.com/skin/base/common/ico_title.gif") no-repeat 0 6px; }
.xans-myshop-asyncbankbook .data { float:left; width:35%; padding:0 10px 0 10px;  text-align:right; }
.xans-myshop-asyncbankbook .use { color:#008bcc; }
.xans-myshop-asyncbankbook a.button { width:25%; }